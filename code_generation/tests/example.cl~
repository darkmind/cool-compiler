
(*  Example cool program testing as many aspects of the code generator
    as possible.
 *)

class Main {
  hw:Helloworld <- (new Helloworld);
  main():Int {
    hw.say()
  };
};

class Helloworld inherits IO {
  say():Int {
    {
      out_string("hello world");
      1;
    }
  };
};

class Myclass {
  hello():Int {
    1
  };
};
